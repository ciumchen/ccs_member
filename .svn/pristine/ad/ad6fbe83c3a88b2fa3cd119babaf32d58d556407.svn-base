.mui-bar-nav{ /* Safari 5.1 - 6.0 */  background: -webkit-gradient(linear, 100% 0, 0 0, from(#e75b0e), to(#eb3d3e));  background: -webkit-linear-gradient(right, #e75b0e, #eb3d3e);  background: -moz-linear-gradient(right, #e75b0e, #eb3d3e);  background: -o-linear-gradient(right, #e75b0e, #eb3d3e);  background: linear-gradient(right, #e75b0e, #eb3d3e); /* Opera 11.1 - 12.0 */   /* Firefox 3.6 - 15 */  /* 标准的语法 */ }.mui-bar-nav h1{	color: #FFF;}.mui-bar-nav .mui-pull-left{	color: #FFF;}.mui-slider-title-tap{	display: flex;}.mui-slider-title-tap>div{	flex: 1;	text-align: center;	padding: 10px 0;	background-color: #FFF;	font-size: 15px;	color: #222222;}.mui-slider-title-tap>div.active>span{	color: #e9242c;    border-bottom: 2px solid #e9242c;    padding-bottom: 10px;}.hide-class{	display: none;}.mui-card-content {    padding-bottom: 10px;}.mui-card-content>div:first-child{	padding-top: 20px;}.last-div{    margin-bottom: 10px;}.mui-card-content>div{	padding: 10px 20px 0 20px;	display: flex;    line-height: 40px;}.mui-card-content>div input{	margin-bottom: 0;	padding: 0 !important;	border: 0;    border-bottom: 1px solid #010101;    border-radius: 0;    height: 30px;    font-size: 14px;    color: #000;}.ruo-bottom input{	border-bottom: 1px solid #a0a0a0 !important;}input::-webkit-input-placeholder{    color:#d1d1d1;}.gray-color-cass input::-moz-placeholder{   /* Mozilla Firefox 19+ */    color:#d1d1d1;}.gray-color-cass input:-moz-placeholder{    /* Mozilla Firefox 4 to 18 */    color:#d1d1d1;}.gray-color-cass input:-ms-input-placeholder{  /* Internet Explorer 10-11 */     color:#d1d1d1;}.mui-card-content .blue-label{	color: #f21910;}.label-div{	flex: 1;    text-align: right;    color: #6e6e6e;}.content-div{	flex: 2;}.blank-color{	color: #010101;}.blank-botom{	border-bottom: 1px solid #010101;}/***********************/.submit-applacation{	background-color: #e75b0f;	color: #FFF;	border-radius: 5px;	width: 80%;    margin-left: 10%;    height: 35px;    line-height: 35px;    text-align: center;    font-size: 17px;}.mui-table-view-cell.mui-active{	background-color: #FFF;}.mui-checkbox input[type=checkbox], .mui-radio input[type=radio]{	width: 20px;    height: 18px;}.mui-checkbox input[type=checkbox]:before, .mui-radio input[type=radio]:before{	font-size: 20px;}.mui-checkbox.mui-left input[type=checkbox], .mui-radio.mui-left input[type=radio]{	margin-top: 2px;}.mui-checkbox input[type=checkbox]:checked:before, .mui-radio input[type=radio]:checked:before{	color: #e75b0f;}.user-xieyi-line{	font-size: 12px;	color: #7c7c7c;	margin-left: -12px;}.user-xieyi{	color: #17b4ee;}